---
import Layout from '../layouts/Layout.astro';

const pageTitle = "SEO Services | WPNurture - Rank Higher on Google";
const pageDescription = "Professional SEO services for business websites, WooCommerce & Shopify stores. Keyword research, on-page optimization, link building, technical SEO. US-based team. Get a free audit.";

const seoFeatures = [
  {
    icon: `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="11" cy="11" r="8"/><path d="m21 21-4.3-4.3"/></svg>`,
    title: "Keyword Research",
    description: "In-depth keyword analysis to find high-intent, low-competition opportunities that drive qualified traffic."
  },
  {
    icon: `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z"/><polyline points="14 2 14 8 20 8"/><line x1="16" x2="8" y1="13" y2="13"/><line x1="16" x2="8" y1="17" y2="17"/><line x1="10" x2="8" y1="9" y2="9"/></svg>`,
    title: "On-Page Optimization",
    description: "Title tags, meta descriptions, header structure, internal linking, and content optimization for every page."
  },
  {
    icon: `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M10 13a5 5 0 0 0 7.54.54l3-3a5 5 0 0 0-7.07-7.07l-1.72 1.71"/><path d="M14 11a5 5 0 0 0-7.54-.54l-3 3a5 5 0 0 0 7.07 7.07l1.71-1.71"/></svg>`,
    title: "Link Building",
    description: "Quality backlink acquisition through outreach, guest posts, and digital PR to boost domain authority."
  },
  {
    icon: `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M3 3v18h18"/><path d="m19 9-5 5-4-4-3 3"/></svg>`,
    title: "Rank Tracking",
    description: "Weekly ranking reports for all target keywords with competitor comparison and trend analysis."
  },
  {
    icon: `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect width="18" height="18" x="3" y="3" rx="2"/><path d="M3 9h18"/><path d="M9 21V9"/></svg>`,
    title: "Technical SEO",
    description: "Site speed optimization, schema markup, XML sitemaps, robots.txt, and Core Web Vitals improvements."
  },
  {
    icon: `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20 10c0 6-8 12-8 12s-8-6-8-12a8 8 0 0 1 16 0Z"/><circle cx="12" cy="10" r="3"/></svg>`,
    title: "Local SEO",
    description: "Google Business Profile optimization, local citations, review management, and local keyword targeting."
  }
];

const packages = [
  {
    name: "Business SEO",
    type: "For Business Websites",
    price: "499",
    description: "Perfect for service businesses, agencies, and corporate websites looking to dominate local and national search.",
    color: "blue",
    features: [
      "Up to 30 target keywords",
      "10 pages optimized/month",
      "4 blog posts/month (1000+ words)",
      "5 quality backlinks/month",
      "Google Business Profile setup",
      "Monthly SEO audit report",
      "Competitor analysis",
      "Schema markup implementation",
      "Core Web Vitals optimization",
      "Bi-weekly strategy calls"
    ],
    ideal: ["Law Firms", "Medical Practices", "Real Estate", "B2B Services", "Consulting Firms"]
  },
  {
    name: "E-commerce SEO",
    type: "For WooCommerce Stores",
    price: "699",
    description: "Comprehensive SEO for WooCommerce stores. Optimized for product visibility, category rankings, and conversions.",
    color: "purple",
    popular: true,
    features: [
      "Up to 50 target keywords",
      "25 product pages optimized/month",
      "Category page optimization",
      "Product schema markup",
      "6 blog posts/month (1500+ words)",
      "8 quality backlinks/month",
      "WooCommerce technical SEO",
      "Site speed optimization",
      "Conversion rate optimization tips",
      "Weekly strategy calls"
    ],
    ideal: ["Fashion Stores", "Electronics", "Home & Garden", "Health & Beauty", "Sports & Outdoors"]
  },
  {
    name: "Shopify SEO",
    type: "For Shopify Stores",
    price: "749",
    description: "Specialized SEO for Shopify stores. We know the platform inside-out and can maximize your store's visibility.",
    color: "green",
    features: [
      "Up to 60 target keywords",
      "30 product pages optimized/month",
      "Collection page optimization",
      "Shopify-specific schema markup",
      "8 blog posts/month (1500+ words)",
      "10 quality backlinks/month",
      "Shopify speed optimization",
      "App recommendations for SEO",
      "Shopify theme SEO fixes",
      "Weekly strategy calls",
      "Shopify Markets SEO (international)"
    ],
    ideal: ["DTC Brands", "Dropshipping", "Print on Demand", "Subscription Boxes", "Luxury Products"]
  }
];

const process = [
  {
    step: "1",
    title: "SEO Audit",
    description: "We analyze your current rankings, technical issues, content gaps, and competitor landscape.",
    icon: `<svg xmlns="http://www.w3.org/2000/svg" class="w-7 h-7" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z" /></svg>`
  },
  {
    step: "2",
    title: "Strategy Development",
    description: "Custom SEO roadmap with prioritized actions, target keywords, and content calendar.",
    icon: `<svg xmlns="http://www.w3.org/2000/svg" class="w-7 h-7" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01" /></svg>`
  },
  {
    step: "3",
    title: "Implementation",
    description: "Our team executes on-page optimizations, technical fixes, and content creation.",
    icon: `<svg xmlns="http://www.w3.org/2000/svg" class="w-7 h-7" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 20l4-16m4 4l4 4-4 4M6 16l-4-4 4-4" /></svg>`
  },
  {
    step: "4",
    title: "Link Building",
    description: "Strategic outreach to acquire high-quality backlinks from relevant websites.",
    icon: `<svg xmlns="http://www.w3.org/2000/svg" class="w-7 h-7" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1" /></svg>`
  },
  {
    step: "5",
    title: "Monitor & Optimize",
    description: "Continuous tracking, reporting, and strategy refinement based on results.",
    icon: `<svg xmlns="http://www.w3.org/2000/svg" class="w-7 h-7" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" /></svg>`
  }
];

const faqs = [
  {
    question: "How long does it take to see SEO results?",
    answer: "Most clients start seeing measurable improvements within 90 days. However, SEO is a long-term strategy. Significant rankings and traffic increases typically occur between 4-6 months, with compounding results over time. We provide monthly reports so you can track progress from day one."
  },
  {
    question: "What's the difference between Business SEO and E-commerce SEO?",
    answer: "Business SEO focuses on service pages, local search, and lead generation for service-based businesses. E-commerce SEO (WooCommerce/Shopify) targets product pages, category optimization, product schema markup, and conversion optimization specific to online stores. E-commerce requires more pages and specialized technical knowledge."
  },
  {
    question: "Do you guarantee first page rankings?",
    answer: "We guarantee measurable improvements and a strategic approach, but no ethical SEO agency can guarantee specific rankings—Google's algorithm considers 200+ factors. What we do guarantee: transparent reporting, proven methodologies, and dedicated effort toward your ranking goals. Most clients see top 10 rankings for target keywords within 6 months."
  },
  {
    question: "What if I already have an in-house team?",
    answer: "We work seamlessly with in-house teams. Many clients use us for specialized tasks like link building, technical audits, or content strategy while their team handles day-to-day work. We can also provide training and consulting to upskill your existing team."
  },
  {
    question: "How do you build backlinks?",
    answer: "We use only white-hat link building methods: digital PR, guest posting on relevant industry sites, resource link building, broken link building, and HARO responses. We never buy links from PBNs or use spammy tactics that could harm your site. Every backlink is from a real, relevant website."
  },
  {
    question: "Can I cancel anytime?",
    answer: "Yes, we offer month-to-month contracts with no long-term commitment required. We recommend at least 6 months to see meaningful results, but you're free to cancel with 30 days notice. We believe our results will speak for themselves."
  }
];

---

<Layout title={pageTitle} description={pageDescription}>
  
  <!-- Breadcrumb Schema + Service Schema + FAQ Schema -->
  <script type="application/ld+json" set:html={JSON.stringify({
    "@context": "https://schema.org",
    "@graph": [
      {
        "@type": "BreadcrumbList",
        "itemListElement": [
          {
            "@type": "ListItem",
            "position": 1,
            "name": "Home",
            "item": "https://wpnurture.com/"
          },
          {
            "@type": "ListItem",
            "position": 2,
            "name": "SEO Services",
            "item": "https://wpnurture.com/seo"
          }
        ]
      },
      {
        "@type": "Service",
        "serviceType": "Search Engine Optimization",
        "provider": {
          "@type": "Organization",
          "name": "WPNurture",
          "url": "https://wpnurture.com"
        },
        "areaServed": {
          "@type": "Country",
          "name": "United States"
        },
        "hasOfferCatalog": {
          "@type": "OfferCatalog",
          "name": "SEO Services",
          "itemListElement": [
            {
              "@type": "Offer",
              "itemOffered": {
                "@type": "Service",
                "name": "Business SEO",
                "description": "SEO for service businesses, agencies, and corporate websites"
              },
              "price": "499",
              "priceCurrency": "USD",
              "priceSpecification": {
                "@type": "UnitPriceSpecification",
                "price": "499",
                "priceCurrency": "USD",
                "billingDuration": "P1M"
              }
            },
            {
              "@type": "Offer",
              "itemOffered": {
                "@type": "Service",
                "name": "WooCommerce SEO",
                "description": "Comprehensive SEO for WooCommerce e-commerce stores"
              },
              "price": "699",
              "priceCurrency": "USD",
              "priceSpecification": {
                "@type": "UnitPriceSpecification",
                "price": "699",
                "priceCurrency": "USD",
                "billingDuration": "P1M"
              }
            },
            {
              "@type": "Offer",
              "itemOffered": {
                "@type": "Service",
                "name": "Shopify SEO",
                "description": "Specialized SEO services for Shopify stores"
              },
              "price": "749",
              "priceCurrency": "USD",
              "priceSpecification": {
                "@type": "UnitPriceSpecification",
                "price": "749",
                "priceCurrency": "USD",
                "billingDuration": "P1M"
              }
            }
          ]
        }
      },
      {
        "@type": "FAQPage",
        "mainEntity": faqs.map(faq => ({
          "@type": "Question",
          "name": faq.question,
          "acceptedAnswer": {
            "@type": "Answer",
            "text": faq.answer
          }
        }))
      }
    ]
  })} />

  <!-- Hero Section -->
  <section class="pt-32 pb-20 px-4 sm:px-6 lg:px-8 bg-gradient-to-b from-slate-900 to-slate-800 text-white">
    <div class="max-w-7xl mx-auto">
      <div class="text-center max-w-4xl mx-auto">
        <!-- Breadcrumb -->
        <nav class="flex justify-center mb-6" aria-label="Breadcrumb">
          <ol class="flex items-center gap-2 text-sm text-slate-400">
            <li><a href="/" class="hover:text-white transition-colors">Home</a></li>
            <li><svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg></li>
            <li class="text-white">SEO Services</li>
          </ol>
        </nav>

        <div class="inline-flex items-center gap-2 bg-white/10 text-white px-4 py-2 rounded-full text-sm font-medium mb-6 backdrop-blur-sm">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="m22 2-7 20-4-9-9-4Z"/><path d="M22 2 11 13"/></svg>
          Rank Higher • Get More Traffic • Grow Revenue
        </div>
        <h1 class="text-4xl sm:text-5xl lg:text-6xl font-extrabold leading-tight mb-6">
          Professional <span class="text-transparent bg-clip-text bg-gradient-to-r from-blue-400 to-cyan-400">SEO Services</span> That Drive Real Results
        </h1>
        <p class="text-xl text-slate-300 mb-10 max-w-2xl mx-auto">
          Stop paying for SEO that doesn't deliver. Our US-based team uses data-driven strategies to help businesses, WooCommerce, and Shopify stores dominate Google search results.
        </p>
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="#packages" class="inline-flex items-center justify-center gap-2 bg-blue-600 text-white px-8 py-4 rounded-xl font-semibold text-lg hover:bg-blue-700 transition-all hover:shadow-lg hover:shadow-blue-600/25">
            View SEO Packages
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" class="w-5 h-5">
              <path stroke-linecap="round" stroke-linejoin="round" d="M19.5 13.5 12 21m0 0-7.5-7.5M12 21V3" />
            </svg>
          </a>
          <a href="#contact" class="inline-flex items-center justify-center gap-2 bg-white/10 text-white px-8 py-4 rounded-xl font-semibold text-lg border border-white/20 hover:bg-white/20 transition-all backdrop-blur-sm">
            Get Free SEO Audit
          </a>
        </div>
      </div>

      <!-- Stats -->
      <div class="mt-20 grid grid-cols-2 md:grid-cols-4 gap-8 max-w-4xl mx-auto">
        <div class="text-center">
          <div class="text-3xl sm:text-4xl font-bold text-white">150%</div>
          <div class="text-slate-400 mt-1">Avg. Traffic Increase</div>
        </div>
        <div class="text-center">
          <div class="text-3xl sm:text-4xl font-bold text-white">Top 10</div>
          <div class="text-slate-400 mt-1">Ranking Guarantee</div>
        </div>
        <div class="text-center">
          <div class="text-3xl sm:text-4xl font-bold text-white">200+</div>
          <div class="text-slate-400 mt-1">Keywords Ranked</div>
        </div>
        <div class="text-center">
          <div class="text-3xl sm:text-4xl font-bold text-white">90 Days</div>
          <div class="text-slate-400 mt-1">Avg. Time to Results</div>
        </div>
      </div>
    </div>
  </section>

  <!-- Trust Badges -->
  <section class="py-8 px-4 sm:px-6 lg:px-8 bg-white border-b border-slate-200">
    <div class="max-w-5xl mx-auto">
      <div class="flex flex-wrap justify-center items-center gap-8 md:gap-12 text-slate-400">
        <div class="flex items-center gap-2">
          <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-green-500" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" /></svg>
          <span class="text-sm font-medium text-slate-600">White-Hat Only</span>
        </div>
        <div class="flex items-center gap-2">
          <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-blue-500" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M3 21v-4m0 0V5a2 2 0 012-2h6.5l1 1H21l-3 6 3 6h-8.5l-1-1H5a2 2 0 00-2 2zm9-13.5V9" /></svg>
          <span class="text-sm font-medium text-slate-600">US-Based Team</span>
        </div>
        <div class="flex items-center gap-2">
          <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-purple-500" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" /></svg>
          <span class="text-sm font-medium text-slate-600">Transparent Reporting</span>
        </div>
        <div class="flex items-center gap-2">
          <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-yellow-500" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M11.049 2.927c.3-.921 1.603-.921 1.902 0l1.519 4.674a1 1 0 00.95.69h4.915c.969 0 1.371 1.24.588 1.81l-3.976 2.888a1 1 0 00-.363 1.118l1.518 4.674c.3.922-.755 1.688-1.538 1.118l-3.976-2.888a1 1 0 00-1.176 0l-3.976 2.888c-.783.57-1.838-.197-1.538-1.118l1.518-4.674a1 1 0 00-.363-1.118l-3.976-2.888c-.784-.57-.38-1.81.588-1.81h4.914a1 1 0 00.951-.69l1.519-4.674z" /></svg>
          <span class="text-sm font-medium text-slate-600">No Long-Term Contracts</span>
        </div>
      </div>
    </div>
  </section>

  <!-- What We Do Section -->
  <section class="py-20 px-4 sm:px-6 lg:px-8 bg-white" id="services">
    <div class="max-w-7xl mx-auto">
      <div class="text-center max-w-3xl mx-auto mb-16">
        <span class="text-blue-600 font-semibold text-sm uppercase tracking-wider">Our SEO Services</span>
        <h2 class="text-3xl sm:text-4xl font-bold text-slate-900 mt-2 mb-4">
          Full-Service SEO Solutions
        </h2>
        <p class="text-lg text-slate-600">
          Everything you need to climb the rankings and stay there. No shortcuts, no black-hat tactics—just proven strategies that deliver lasting results.
        </p>
      </div>
      
      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
        {seoFeatures.map((feature) => (
          <div class="group p-6 bg-slate-50 rounded-2xl hover:bg-white hover:shadow-xl hover:shadow-slate-200/50 transition-all duration-300 border border-transparent hover:border-slate-200">
            <div class="w-12 h-12 bg-gradient-to-br from-blue-600 to-indigo-600 rounded-xl flex items-center justify-center text-white mb-4 group-hover:scale-110 transition-transform">
              <Fragment set:html={feature.icon} />
            </div>
            <h3 class="text-xl font-semibold text-slate-900 mb-2">{feature.title}</h3>
            <p class="text-slate-600 leading-relaxed">{feature.description}</p>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Why Choose Us -->
  <section class="py-20 px-4 sm:px-6 lg:px-8 bg-gradient-to-br from-blue-600 to-indigo-700 text-white">
    <div class="max-w-7xl mx-auto">
      <div class="grid lg:grid-cols-2 gap-12 items-center">
        <div>
          <span class="text-blue-200 font-semibold text-sm uppercase tracking-wider">Why WPNurture</span>
          <h2 class="text-3xl sm:text-4xl font-bold mt-2 mb-6">
            SEO That Actually Moves the Needle
          </h2>
          <p class="text-blue-100 text-lg mb-8">
            We're not just another SEO agency. We're a US-based team of specialists who eat, sleep, and breathe search engine optimization. Here's what makes us different:
          </p>
          <ul class="space-y-4">
            <li class="flex items-start gap-3">
              <div class="w-6 h-6 bg-white/20 rounded-full flex items-center justify-center flex-shrink-0 mt-0.5">
                <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path></svg>
              </div>
              <div>
                <strong class="text-white">Data-Driven Decisions</strong>
                <p class="text-blue-200 text-sm mt-1">Every recommendation is backed by data, not guesswork. We use advanced tools to identify exactly what your site needs.</p>
              </div>
            </li>
            <li class="flex items-start gap-3">
              <div class="w-6 h-6 bg-white/20 rounded-full flex items-center justify-center flex-shrink-0 mt-0.5">
                <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path></svg>
              </div>
              <div>
                <strong class="text-white">Platform Expertise</strong>
                <p class="text-blue-200 text-sm mt-1">Specialized knowledge in WordPress, WooCommerce, and Shopify. We know the quirks and opportunities of each platform.</p>
              </div>
            </li>
            <li class="flex items-start gap-3">
              <div class="w-6 h-6 bg-white/20 rounded-full flex items-center justify-center flex-shrink-0 mt-0.5">
                <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path></svg>
              </div>
              <div>
                <strong class="text-white">Transparent Communication</strong>
                <p class="text-blue-200 text-sm mt-1">Monthly reports, regular calls, and a dedicated account manager. You'll always know what we're doing and why.</p>
              </div>
            </li>

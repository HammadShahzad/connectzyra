  <script>
    // FAQ Accordion
    document.querySelectorAll('.faq-toggle').forEach(button => {
      button.addEventListener('click', () => {
        const content = button.nextElementSibling;
        const icon = button.querySelector('.faq-icon');
        
        // Close all other FAQs
        document.querySelectorAll('.faq-content').forEach(c => {
          if (c !== content) {
            c.classList.add('hidden');
            c.previousElementSibling?.querySelector('.faq-icon')?.classList.remove('rotate-180');
          }
        });
        
        // Toggle current FAQ
        content?.classList.toggle('hidden');
        icon?.classList.toggle('rotate-180');
      });
    });

    // Reviews Slider Logic
    const seoSlider = document.getElementById('seo-reviews-slider');
    const seoPrevBtn = document.getElementById('seo-prev-review');
    const seoNextBtn = document.getElementById('seo-next-review');
    const seoCounter = document.getElementById('seo-slide-counter');
    
    let seoCurrentSlide = 0;
    const seoTotalSlides = 6;

    function seoScrollToSlide(index: number) {
      if (!seoSlider) return;
      
      // Handle looping
      if (index < 0) index = seoTotalSlides - 1;
      if (index >= seoTotalSlides) index = 0;
      
      seoCurrentSlide = index;
      const slideWidth = seoSlider.querySelector('div')?.offsetWidth || 380;
      seoSlider.scrollTo({ left: index * (slideWidth + 24), behavior: 'smooth' });
      if (seoCounter) seoCounter.textContent = `${index + 1} / ${seoTotalSlides}`;
    }

    seoPrevBtn?.addEventListener('click', () => seoScrollToSlide(seoCurrentSlide - 1));
    seoNextBtn?.addEventListener('click', () => seoScrollToSlide(seoCurrentSlide + 1));

    // Auto-advance every 5 seconds
    setInterval(() => seoScrollToSlide(seoCurrentSlide + 1), 5000);

    // Form submission
    const seoForm = document.getElementById('seo-form') as HTMLFormElement;
    const seoSuccess = document.getElementById('seo-success');
    const seoBtnText = document.getElementById('seo-btn-text');
    const seoBtnSpinner = document.getElementById('seo-btn-spinner');

    seoForm?.addEventListener('submit', async (e) => {
      e.preventDefault();
      
      seoBtnText?.classList.add('hidden');
      seoBtnSpinner?.classList.remove('hidden');

      try {
        const formData = new FormData(seoForm);
        const response = await fetch(seoForm.action, {
          method: 'POST',
          body: formData
        });

        if (response.ok) {
          seoForm.classList.add('hidden');
          seoSuccess?.classList.remove('hidden');
        } else {
          throw new Error('Form submission failed');
        }
      } catch (error) {
        alert('Something went wrong. Please try again.');
        seoBtnText?.classList.remove('hidden');
        seoBtnSpinner?.classList.add('hidden');
      }
    });
  </script>

